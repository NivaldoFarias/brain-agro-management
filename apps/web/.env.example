# Brain Agriculture Web Application Environment Variables
# Copy this file to .env and fill in your values

# Node.js Environment (development, production, test)
NODE_ENV=development

# Web Application Configuration
WEB__LOG_LEVEL=info
WEB__LOG_TO_CONSOLE=true
WEB__PORT=5173
WEB__HOST=localhost
WEB__PREVIEW_PORT=4173

# Vite Environment Variables (exposed to browser)
WEB__VITE_API_BASE_URL=http://localhost:3000/api
WEB__VITE_ENABLE_DEVTOOLS=false
